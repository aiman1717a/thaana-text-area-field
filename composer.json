{
    "name": "aiman/thaana-text-area-field",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova",
        "thaana",
        "jtk",
        "text area",
        "text"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Aiman\\ThaanaTextAreaField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aiman\\ThaanaTextAreaField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
